        <div class="col-xs-12 col-sm-12 col-md-9 col-truyen-main">
            <div class="text-center"></div>
            <div class="list list-truyen col-xs-12">
                <div class="title-list">
                    <h2>Truyện Kiếm Hiệp</h2>
                    <div class="filter"><a href="https://truyenfull.vn/the-loai/kiem-hiep/hoan/">Kiếm Hiệp hoàn
                            (full)</a>
                    </div>
                </div>
                 {{#each list}}
                        <div class="row" itemscope="" itemtype="https://schema.org/Book">
                    <div class="col-xs-3">
                        <div><img
                                src="{{ this.url_hinh }}"
                                class="cover" alt="{{ this.ten_truyen }}"></div>
                    </div>
                    <div class="col-xs-7">
                        <div><span class="glyphicon glyphicon-book"></span>
                            <h3 class="truyen-title" itemprop="name"><a href="https://truyenfull.vn/that-nguyet/"
                                    title="{{ this.ten_truyen }}" itemprop="url">{{this.ten_truyen}}</a></h3>
                        {{#if this.trang_thai "eq" "Full" }}
                        <span class="label-full"></span>
                        {{/if}}
                        {{#if this.danh_gia "eq" "1" }}
                        <span class="label-hot"></span>
                        {{/if}}
                                <span class="author" itemprop="author">
                                        <span
                                    class="glyphicon glyphicon-pencil"></span>{{this.tac_gia}}</span>
                        </div>
                    </div>
                    <div class="col-xs-2 text-info">
                        <div><a href="{{this.url_truyen}}"
                                title="{{this.so_chuong}}">
                                <span class="chapter-text"><span>Chương </span></span>{{this.so_chuong}}</a></div>
                    </div>
                </div>
                    {{/each}}
            </div>
        </div>
